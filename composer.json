{
    "name": "isaacongoma/laravel-analytics",
    "description": "A Laravel package to retrieve Google Analytics data.",
    "keywords": [
        "spatie",
        "google",
        "analytics",
        "retrieve",
        "reports",
        "laravel"
    ],
    "homepage": "https://github.com/spatie/laravel-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "google/apiclient": "*",
        "nesbot/carbon": "*",
        "spatie/laravel-package-tools": "*",
        "symfony/cache": "*"
    },
    "require-dev": {
        "league/flysystem": "*",
        "mockery/mockery": "*",
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Analytics\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Analytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "Spatie\\Analytics\\AnalyticsFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Analytics\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
